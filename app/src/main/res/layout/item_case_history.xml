<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/item_caseHistory"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    >


    <ImageView
        android:id="@+id/item_imageView_caseHistory"
        android:layout_height="80dp"
        android:layout_width="80dp"
        app:srcCompat="@android:drawable/ic_notification_clear_all"
        />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:id="@+id/item_textView_caseHistory_ID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="id"
            android:visibility="invisible"/>
        <TextView
            android:id="@+id/item_textView_caseHistory_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="nonononononononononono"/>
        <TextView
            android:id="@+id/item_textView_caseHistory_image_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="id"
            android:visibility="invisible"/>
        <TextView
            android:id="@+id/item_textView_caseHistory_time_insert"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="yes"
            android:visibility="visible"/>
        <TextView
            android:id="@+id/item_textView_caseHistory_time_update"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="yes"
            android:visibility="invisible"/>
        <TextView
            android:id="@+id/item_textView_caseHistory_time_timeID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="yes"
            android:visibility="invisible"/>
    </LinearLayout>

</LinearLayout>